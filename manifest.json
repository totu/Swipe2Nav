{
  "content_scripts": [
    {
      "js": [
        "jquery-3.4.1.js",
        "jquery.mousewheel.js",
        "swipe2nav.js"
      ],
      "matches": ["*://*/*"]
    }
  ],

  "options_ui": {
    "page": "options.html"
  },

  "permissions": ["storage"],

  "browser_specific_settings": {
    "gecko": {
      "id": "swipe2nav@localhost"
    }
  },
  "description": "Add two finger swip for navigating browser history.",
  "name": "SwipeToNavigate",
  "version": "0.2.0",
  "manifest_version": 2
}
